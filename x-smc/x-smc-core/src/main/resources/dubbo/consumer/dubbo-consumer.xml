<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans     
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd 
           http://code.alibabatech.com/schema/dubbo
           http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	<bean id="dubboConfig"
		class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
		<property name="order" value="1" />
		<property name="ignoreUnresolvablePlaceholders" value="true" />
		<!-- <property name="locations" value="classpath:dubbo/dubbo.properties" /> -->
	</bean>
	<dubbo:application name="${dubbo.appname}" />

	<dubbo:registry id="registry" protocol="${dubbo.registry.protocol}"
		address="${dubbo.registry.address}"  />
	<dubbo:monitor protocol="registry" />

	<import resource="reference/baas-dshm-dubbo-reference.xml" />
</beans>
